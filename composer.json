{
    "name":"livestreet/framework",
    "license": "MIT",
    "description": "Livestreet Framework",
    "authors": [
      {"name": "OlegDemidov"}
    ],
    
    "require": {
        "livestreet/asset": "dev-master",
        "agelxnash/jevix": "1.2.4",
        "livestreet/smarty": "dev-master",
        "livestreet/dbsimple": "dev-master",
        "maximebf/consolekit": ">=1.0.0",
        "livestreet/dklabcache": ">=1.0.1",
        "imagine/imagine": "^1.2",
        "monolog/monolog": "1.24.0",
        "patchwork/jsqueeze": "~2.0",
        "xxtea/xxtea": "^1.0",
        "natxet/cssmin": "v3.0.6",
        "livestreet/forker": "dev-master",
        "ganlvtech/kcaptcha": "1.0.1",
        "aura/autoload": "2.0.4",
        "phpmailer/phpmailer": "~6.0",
        "guzzlehttp/psr7":"1.6.0",
        "narrowspark/http-emitter":"0.8.0",
        "olegdemidov/composer-packages":"dev-master",
        "ckr/arraymerger":"2.0.0"
    },
    "autoload": {
        "psr-4" : {
            "LS\\": "classes/"
        },
        "classmap":[
            "classes/engine/"
        ],
        "files": [
            "bootstrap/start.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "7.5.13"
    }
}
